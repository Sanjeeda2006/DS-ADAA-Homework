#include<stdio.h>

int main() {
    int arr[100];
    int n, i, j, k;

    // Input array size
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Input elements
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Remove duplicates in-place
    for(i = 0; i < n; i++) {
        for(j = i + 1; j < n; ) {
            if(arr[i] == arr[j]) {
                // Shift elements left to remove duplicate
                for(k = j; k < n - 1; k++) {
                    arr[k] = arr[k + 1];
                }
                n--; // reduce array size
            } else {
                j++;
            }
        }
    }

    // Print array with unique elements
    printf("\nArray with unique elements:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}