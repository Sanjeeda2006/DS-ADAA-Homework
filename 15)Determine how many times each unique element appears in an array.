#include<stdio.h>

int main() {
    int arr[100], freq[100];
    int n, i, j, count;

    // Input array size
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Input array elements
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        freq[i] = -1;  // initially mark as not counted
    }

    // Count frequency
    for(i = 0; i < n; i++) {
        count = 1;
        for(j = i + 1; j < n; j++) {
            if(arr[i] == arr[j]) {
                count++;
                freq[j] = 0;  // mark duplicate as counted
            }
        }
        if(freq[i] != 0) {   // if not counted already
            freq[i] = count;
        }
    }

    // Display frequencies
    printf("\nFrequency of each unique element:\n");
    for(i = 0; i < n; i++) {
        if(freq[i] != 0) {
            printf("%d appears %d times\n", arr[i], freq[i]);
        }
    }

    return 0;
}